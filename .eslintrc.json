{
  "extends": [
    "eslint:recommended",
    "prettier",
    "prettier/react",
    "plugin:import/errors",
    "plugin:react/recommended"
  ],
  "plugins": ["react", "import"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
